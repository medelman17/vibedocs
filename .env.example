# .env.example
# Database
DATABASE_URL="postgresql://..."

# Auth.js
AUTH_SECRET="generate-with-openssl-rand-base64-32"
AUTH_GOOGLE_ID="your-google-client-id"
AUTH_GOOGLE_SECRET="your-google-client-secret"
AUTH_GITHUB_ID="your-github-client-id"
AUTH_GITHUB_SECRET="your-github-client-secret"

# Resend (email)
RESEND_API_KEY="re_..."

# Vercel Blob (already configured)
BLOB_READ_WRITE_TOKEN="..."

# =============================================================================
# Inngest - Durable Workflow Orchestration
# =============================================================================
# Get keys from: https://app.inngest.com/env/{env}/manage/keys

# Set to 1 for local development (connects SDK to local dev server)
INNGEST_DEV=1

# Event key for sending events to Inngest (use dummy value locally)
INNGEST_EVENT_KEY=local-dev-key

# Signing key for webhook signature verification (use dummy value locally)
INNGEST_SIGNING_KEY=local-signing-key
